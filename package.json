{
    "name": "mmo-batch-data-process-svc",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "engines": {
        "node": "~18.12.1",
        "npm": "~8.19.2"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "config.ts",
            "**/*.js",
            "**/*.d.ts",
            "dist",
            "mock_data",
            "coverage",
            "test",
            "src/validators/interfaces",
            "src/session_store/storeable.ts",
            "src/app_settings.ts",
            "src/start.ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true,
        "check-coverage": true
    },
    "scripts": {
        "test": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=test-results.xml NODE_ENV=test jest test/ --coverage --ci",
        "test:ci": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=junit.xml NODE_ENV=test jest --runInBand --watchAll=false --reporters=default --reporters=jest-junit --coverage --ci",
        "test:watch": "cross-env JEST_JUNIT_OUTPUT_DIR=test-results JEST_JUNIT_OUTPUT_NAME=test-results.xml NODE_ENV=test jest --runInBand test/ --coverage --ci --watch",
        "start": "cross-env NODE_ENV=development nodemon -w src/ -x ts-node src/start.ts",
        "start-debug": "node --inspect ./dist/start.js",
        "build": "tsc",
        "debug": "cross-env NODE_ENV=development npm-run-all build start-debug --print-label",
        "lint": "eslint . --ext .ts"
    },
    "devDependencies": {
        "@types/bunyan": "1.8.11",
        "@types/csvtojson": "2.0.0",
        "@types/hapi": "^18.0.14",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.1",
        "@types/node": "20.12.11",
        "@typescript-eslint/eslint-plugin": "7.8.0",
        "@typescript-eslint/parser": "7.8.0",
        "babel-jest": "^29.7.0",
        "cross-env": "7.0.3",
        "eslint": "8.57.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-plugin-jest": "28.5.0",
        "eslint-plugin-prettier": "5.1.3",
        "husky": "9.0.11",
        "jest": "29.7.0",
        "jest-junit": "16.0.0",
        "mongodb-memory-server": "9.2.0",
        "nodemon": "3.1.0",
        "npm-run-all": "4.1.5",
        "prettier": "3.2.5",
        "sinon": "17.0.1",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "5.4.5"
    },
    "dependencies": {
        "@azure/service-bus": "^7.9.4",
        "@hapi/basic": "^7.0.2",
        "@hapi/hapi": "^21.3.9",
        "ajv": "8.13.0",
        "ajv-formats": "^3.0.1",
        "ajv-keywords": "^5.1.0",
        "applicationinsights": "2.9.5",
        "applicationinsights-native-metrics": "0.0.10",
        "axios": "1.6.8",
        "azure-storage": "^2.10.7",
        "bunyan": "1.8.15",
        "cross-env": "7.0.3",
        "csvtojson": "2.0.10",
        "dotenv": "^16.4.5",
        "joi": "^17.13.1",
        "lodash": "^4.17.21",
        "mingo": "^6.4.15",
        "mmo-shared-reference-data": "1.0.36",
        "moment": "^2.30.1",
        "mongodb": "6.6.1",
        "mongoose": "^8.3.4",
        "node-cron": "^3.0.3",
        "url-parse": "^1.5.10",
        "uuid": "^9.0.1"
    },
    "jest": {
        "moduleNameMapper": {
            "^axios/": "axios/dist/node/axios.cjs"
        },
        "testEnvironment": "node",
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.ts?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "tsx",
            "ts",
            "js",
            "json"
        ],
        "testRegex": "/test/.*\\.(tsx|ts|js)$",
        "testPathIgnorePatterns": [
            "/test/mockData.js"
        ],
        "coverageReporters": [
            "clover",
            "json",
            "lcov",
            "text",
            "cobertura"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 100,
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        }
    },
    "prettier": {
        "singleQuote": true
    },
    "author": "",
    "license": "ISC"
}